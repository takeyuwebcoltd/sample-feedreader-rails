<% if @channel.errors.any? %>
  <div id="error_explanation">
    <h2><%= @channel.errors.count %>件のエラーがあります。</h2>
    <ul>
      <% @channel.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>

<%= form_for(@channel) do |f| %>
  <%= f.label :url %><br />
  <%= f.text_field :url %>
  <%= f.submit "登録" %>
<% end %>
